{
  "name": "@coolgk/utils",
  "version": "2.1.3",
  "author": "Daniel Gong <daniel.k.gong@gmail.com>",
  "homepage": "https://www.npmjs.com/package/@coolgk/utils",
  "bugs": {
    "url": "https://github.com/coolgk/node-utils/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coolgk/node-utils.git"
  },
  "description": "javascript, typescript utility and wrapper functions and classes: array, string, base64, ampq, bcrypt, cache, captcha, csv, email, jwt, number, pdf, tmp, token, unit conversion, url params, session, form data",
  "keywords": [
    "array",
    "string",
    "base64",
    "ampq",
    "bcrypt",
    "cache",
    "captcha",
    "csv",
    "email",
    "jwt",
    "number",
    "pdf",
    "typescript",
    "tmp",
    "token",
    "unit conversion",
    "url params",
    "session",
    "express",
    "formdata",
    "middleware",
    "multipart",
    "x-www-form-urlencoded",
    "form"
  ],
  "scripts": {
    "pretest": "tsc --project tsconfig.dev.json",
    "test": "export $(cat .env | xargs) && nyc mocha test",
    "test:quick": "export $(cat .env | xargs) && mocha test",
    "test:precommit": "export $(cat .env | xargs) && tsc --project tsconfig.json && nyc --reporter text-summary mocha test --reporter min",
    "scan:local": "nsp check",
    "scan": "ls -d packages/* | xargs -n1 nsp check --reporter summary",
    "tslint": "tslint --exclude src/test.ts src/**/*",
    "tsc": "tsc --project tsconfig.json",
    "tsc:watch": "tsc --project tsconfig.debug.json -w --pretty",
    "tsc:dev": "tsc --project tsconfig.dev.json",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "debug": "export $(cat .env | xargs) && nodemon --watch ./dist --inspect=0.0.0.0:9229 --nolazy ./dist/test.js",
    "package": "gulp package"
  },
  "dependencies": {
    "@coolgk/array": "^2.0.3",
    "@coolgk/base64": "^2.0.3",
    "@coolgk/cache": "^2.0.3",
    "@coolgk/jwt": "^2.0.5",
    "@coolgk/queue": "^2.0.3",
    "@coolgk/string": "^2.0.3",
    "@coolgk/tmp": "^2.0.4",
    "@coolgk/token": "^2.0.3",
    "amqplib": "^0.5.2",
    "bcrypt-nodejs": "0.0.3",
    "busboy": "^0.2.14",
    "cookie": "^0.3.1",
    "csv-parse": "^2.0.2",
    "csv-stringify": "^2.0.2",
    "emailjs": "^2.0.0",
    "mime-types": "^2.1.17",
    "phantom": "^4.0.12",
    "redis": "^2.8.0",
    "request": "^2.83.0",
    "tmp": "0.0.33",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.6",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/busboy": "^0.2.3",
    "@types/cookie": "^0.3.1",
    "@types/csv-parse": "^1.1.11",
    "@types/csv-stringify": "^1.4.2",
    "@types/mime-types": "^2.1.0",
    "@types/mongodb": "^3",
    "@types/node": "^9.4.4",
    "@types/phantom": "^3.2.4",
    "@types/redis": "^2.8.5",
    "@types/request": "^2.47.0",
    "@types/tmp": "0.0.33",
    "@types/uuid": "^3.4.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.3.0",
    "coveralls": "^3.0.0",
    "del": "^3.0.0",
    "eslint": "^4.17.0",
    "eslint-config-google": "^0.9.1",
    "express": "^4.16.2",
    "gulp": "^3.9.1",
    "gulp-header": "^1.8.9",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^3.2.4",
    "js-yaml": "^3.10.0",
    "jsdoc-to-markdown": "^3.0.4",
    "mocha": "^4.1.0",
    "nsp": "^3.1.0",
    "nyc": "^11.4.1",
    "pre-commit": "^1.2.2",
    "retire": "^1.5.1",
    "sinon": "^4.2.2",
    "snyk": "^1.69.7",
    "source-map-support": "^0.5.3",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1"
  },
  "pre-commit": {
    "silent": false,
    "run": [
      "tslint",
      "test:precommit"
    ]
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT"
}
